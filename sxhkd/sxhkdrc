super + Return
    alacritty

super + r
    rofi -show drun -show-icons -drun-icon-theme Papirus -theme /home/ethan/.config/rofi/sidetab.rasi

#Close focused window
super + q
    xdo close

# Restart BSPWM
super + shift + r
    bspc wm -r

super + shift + q
    bspc quit

super + {h,j,k,l}
    bspc node -f {west,south,north,east}

super + {0-9}
    bspc desktop -f {0-9}

super + shift + {0-9}
    bspc node -d {0-9} && bspunhide

super + f
    bspc node -t fullscreen || bspc node -t tiled

super + space
    bspc node -t floating || bspc node -t tiled

super + button1 + drag
    bspc node -t floating

super + shift + {h,j,k,l}
    bspc node -s {west,south,north,east}


